{
  "name": "famous-webpack-seed",
  "version": "0.9.2",
  "description": "A seed project to get started with Webpack and Famo.us",
  "main": "webpack.config.js",
  "scripts": {
    "start": "node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "clean": "rm -rf dist && rm -rf www",
    "remove-cordova": "rm -f config.xml && rm -rf merges && rm -rf platforms && rm -rf www && rm -rf plugins",
    "build": "node_modules/webpack/bin/webpack.js",
    "build-cordova": "node_modules/webpack/bin/webpack.js --config webpack.cordova.js",
    "cordova": "node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.cordova.js",
    "test": "npm run lint -s"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:markmarijnissen/famous-webpack-seed.git"
  },
  "keywords": [
    "famo.us",
    "webpack",
    "seed"
  ],
  "author": "Mark Marijnissen",
  "contributors": [
    {
      "name": "Adrian Rossouw",
      "url": "https://github.com/Vertice/"
    },
    {
      "name": "Tony Alves",
      "url": "https://github.com/talves/"
    }
  ],
  "license": "ISC",
  "devDependencies": {
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.7.1",
    "css-loader": "^0.7.0",
    "expose-loader": "^0.5.3",
    "file-loader": "git://github.com/talves/file-loader.git",
    "glob": "^4.0.5",
    "html-loader": "^0.2.2",
    "jade": "1.2.0",
    "jade-loader": "^0.6.1",
    "json-loader": "^0.5.1",
    "less": "^1.7.4",
    "less-loader": "^0.7.5",
    "optimist": "^0.6.1",
    "script-loader": "^0.6.0",
    "style-loader": "^0.6.4",
    "url-loader": "^0.5.5",
    "webpack": "^1.4.10",
    "webpack-cordova-plugin": "^0.1.5",
    "webpack-dev-server": "^1.6.5",
    "webpack-reload-plugin": "^0.1.1"
  },
  "dependencies": {
    "famous": "^0.3.5",
    "famous-polyfills": "^0.3.0"
  }
}
